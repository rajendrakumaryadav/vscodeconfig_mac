{
    "workbench.colorTheme": "Atom One Dark",
    "editor.fontFamily": "Consolas",
    "editor.fontSize": 18,
    "editor.wordWrap": "on",
    "telemetry.enableCrashReporter": false,
    "telemetry.enableTelemetry": false,
    "terminal.integrated.cursorStyle": "underline",
    "terminal.integrated.enableBell": true,
    "workbench.iconTheme": "material-icon-theme",
    "window.zoomLevel": 0,
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    "git.showInlineOpenFileAction": false,
    "editor.cursorBlinking": "blink",
    "editor.formatOnType": true,
    "workbench.fontAliasing": "auto",
    "editor.formatOnSaveTimeout": 1000,
    "terminal.integrated.fontSize": 16,
    "terminal.integrated.fontFamily": "monaco",
    "terminal.integrated.fontWeightBold": "500",
    "workbench.startupEditor": "newUntitledFile",
    "material-icon-theme.showUpdateMessage": true,
    "terminal.integrated.fontWeight": "500",
    "breadcrumbs.enabled": true,
    "explorer.openEditors.visible": 0,
    "editor.minimap.enabled": false,
    "workbench.statusBar.feedback.visible": false,
    "workbench.statusBar.visible": true,
    "workbench.activityBar.visible": false,
    "php.suggest.basic": false,
    "editor.tabCompletion": true,
    "php.validate.executablePath": "/usr/bin/php",
    "material-icon-theme.opacity": 0.8,
    "extensions.showRecommendationsOnlyOnDemand": true,
    "extensions.ignoreRecommendations": true,
    "emmet.triggerExpansionOnTab": true,
    "blade.format.enable": true,
    "eslint.autoFixOnSave": true,
    "eslint.alwaysShowStatus": true,
    "eslint.validate": [
        "javascript",
        "javascriptreact",
        "vue"
    ],
    "eslint.runtime": null,
    "eslint.nodePath": "/usr/local/node/bin/node",
    "editor.cursorStyle": "underline",
    "editor.foldingStrategy": "indentation",
    "editor.formatOnPaste": true,
    "editor.formatOnSave": true,
    "editor.smoothScrolling": true,
    "editor.stablePeek": true,
    "search.smartCase": true,
    "search.quickOpen.includeSymbols": true,
    "terminal.integrated.shell.osx": "/bin/zsh",
    "outline.icons": false,
    "npm.exclude": "/usr/local/node/bin/npm",
    "intelephense.completionProvider.maxItems": 150,
    "vetur.validation.style": false,
    "vetur.format.scriptInitialIndent": true,
    "vetur.format.styleInitialIndent": true,
    "html.validate.scripts": false,
    "html.validate.styles": false,
    "files.associations": {
        "*.php": "php"
    },
    "intelephense.formatProvider.enable": false,
    "emmet.includeLanguages": {
        "vue-html": "html",
        "javascript": "javascriptreact",
        "typescript": "typescriptreact",
        "blade": "html"
    }
}